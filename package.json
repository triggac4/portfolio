{
    "name": "portfolio",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^12.8.3",
        "aos": "^2.3.4",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.4",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "redux": "^4.1.0",
        "redux-persist": "^6.0.0",
        "web-vitals": "^1.1.2"
    },
    "scripts": {
        "react": "react-scripts start",
        "watch:scss": "node-sass src/main.scss src/App.css -w",
        "start": "npm-run-all --parallel watch:scss react",
        "compile:css": "node-sass src/main.scss src/App.comp.css",
        "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/App.comp.css -o src/App.prefix.css",
        "compress:css": "node-sass src/App.prefix.css src/App.css --output-style compressed",
        "react-build": "react-scripts build",
        "build": "npm-run-all compile:css prefix:css compress:css react-build",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "homepage": "https://triggac4.github.io",
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "gh-pages": "^3.2.3",
        "node-sass": "^6.0.1",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^8.3.1"
    }
}
